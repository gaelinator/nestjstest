{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjstest/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nestjstest/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nestjstest",
  "projects": {
    "nestjstest": {
      "type": "application",
      "root": "apps/nestjstest",
      "entryFile": "main",
      "sourceRoot": "apps/nestjstest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestjstest/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "authentification": {
      "type": "application",
      "root": "apps/authentification",
      "entryFile": "main",
      "sourceRoot": "apps/authentification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentification/tsconfig.app.json"
      }
    },
    "profile-management": {
      "type": "application",
      "root": "apps/profile-management",
      "entryFile": "main",
      "sourceRoot": "apps/profile-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/profile-management/tsconfig.app.json"
      }
    },
    "customer-service": {
      "type": "application",
      "root": "apps/customer-service",
      "entryFile": "main",
      "sourceRoot": "apps/customer-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/customer-service/tsconfig.app.json"
      }
    },
    "measurment-service": {
      "type": "application",
      "root": "apps/measurment-service",
      "entryFile": "main",
      "sourceRoot": "apps/measurment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/measurment-service/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    }
  }
}